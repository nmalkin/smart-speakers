{
    "name": "smart-speakers",
    "version": "0.0.0",
    "dependencies": {},
    "devDependencies": {
        "eslint": "^5.8.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-plugin-import": "^2.14.0",
        "husky": "^1.1.2",
        "lint-staged": "^8.0.0",
        "prettier": "^1.14.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
